config wifi-device  radio0
	option type     mac80211
	option channel  11
	option hwmode	11g
	option path	'platform/bcm2708_usb/usb1/1-1/1-1.2/1-1.2:1.0'
	option htmode	
	# REMOVE THIS LINE TO ENABLE WIFI:
	#option disabled 1

config wifi-iface
	option device   radio0
	option network  wan
	option mode     sta
	option ssid     pgaek	
	option encryption wpa-psk
	option key super-puper-college

