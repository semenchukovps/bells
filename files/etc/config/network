
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'eth0'
	option proto 'dhcp'

config interface 'wan'
#	option ifname 'wlan0'
	option proto 'dhcp'

config globals 'globals'
	option ula_prefix 'fd03:0bbc:05d4::/48'

